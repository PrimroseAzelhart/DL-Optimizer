{
    "manifest_version": 3,
    "name": "DL Optimizer",
    "description": "Make the UX of DLsite better",
    "version": "0.1",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "host_permissions": [
        "*://*.kickstarter.com/*"
    ],
    "permissions": ["scripting", "activeTab", "tabs", "webNavigation", "webRequest"]
}
